# ==========================================
# Bot Configuration
# ==========================================

# The Discord Bot Token
DISCORD_TOKEN=your_token_here
# Alternative token env var if used
ARCHITECT_TOKEN=

# Google Gemini API Key (for Agent Brain)
GOOGLE_API_KEY=your_google_ai_key_here

# ==========================================
# Server Specific Config
# ==========================================

# Unique Identifier for this deployment (e.g. BAD-DEV, BAD-PROD)
SERVER_ID=BAD-PROD

# The Channel ID where the bot listens for commands/logs
AGENT_CHANNEL_ID=

# ==========================================
# Ticket Config (Category/Channel IDs)
# ==========================================

# Where incoming tickets should be routed/categorized
TICKET_MANAGER_INBOX_ID=0
TICKET_INCOMING_ID=0
TICKET_ACTIVE_ID=0
TICKET_BLOCKED_ID=0
TICKET_ARCHIVES_ID=0

# ==========================================
# Backup Configuration (Optional)
# ==========================================
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
BACKUP_BUCKET_NAME=
AWS_REGION=us-east-1

# ==========================================
# Optional / Other
# ==========================================
PLANNING_CHANNEL_ID=0
